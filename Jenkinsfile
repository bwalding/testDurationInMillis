node() {
   stage("stage1") {
       echo "Stage 1 running - getting ${env.SANDWICH}"
       echo "More steps"
   }
   stage("stage2") {
       echo "Stage 2 running - making ${env.SANDWICH} sandwich"
       echo "What does this do?"
   }
   stage("stage3") {
       echo "Stage 3 running - eating ${env.SANDWICH} sandwich"
       echo "Munch munch"
       sh "sleep 15"
   }
}
